{
  "compilerOptions": {
    "target": "es2017",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "src/prisma/index.ts"],
  "exclude": ["node_modules"]
}
